<script>
    $(document).ready(function () {
        var sc = $(".gnbRight").offset();
        //console.log(sc);
        //console.log($(document).scrollTop());

        $(window).scroll(function () {
            //console.log($(document).scrollTop());
            if ($(document).scrollTop() > sc.top) {
                $(".gnbRight").addClass("menu_fixed");
            }
            else {
                $(".gnbRight").removeClass("menu_fixed");
            }
        });
    });

</script>
